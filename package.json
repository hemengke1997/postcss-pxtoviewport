{
  "name": "@minko-fe/postcss-pxtoviewport",
  "version": "0.0.3",
  "description": "✨ A CSS post-processor that converts px to viewport units (vw, vh, vmin, vmax).",
  "type": "module",
  "author": "hemengke1997 <https://github.com/hemengke1997>",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README-en.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "lint": "eslint .",
    "upgrade:deps": "pnpm update --i --L",
    "//": "先bump，再release",
    "bump": "bumpp package.json --commit --push --tag",
    "release": "pnpm run build && pnpm publish --access public"
  },
  "keywords": [
    "pxtoviewport",
    "viewport",
    "postcss",
    "css",
    "pixel",
    "px",
    "postcss-plugin",
    "postcss-px-to-viewport",
    "postcss-pxtoviewport"
  ],
  "license": "MIT",
  "dependencies": {
    "query-string": "^7.1.1"
  },
  "devDependencies": {
    "@minko-fe/commitlint-config": "^1.2.1",
    "@minko-fe/eslint-config-ts": "^1.2.1",
    "@minko-fe/prettier-config": "^1.2.1",
    "@minko-fe/tsconfig": "^1.2.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.25.0",
    "postcss": "^8.4.17",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4",
    "vitest": "^0.24.1"
  }
}
